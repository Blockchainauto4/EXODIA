{
  "redirects": [
    {
      "source": "/tag/(.*)",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/atendimento/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api/|robots\\.txt|sitemap\\.xml|assets/|favicon\\.ico).* )",
      "destination": "/index.html"
    }
  ]
}